<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="535.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pidevArtiste.DisplayArtisteController">
   <children>
      <ListView fx:id="artistelist" layoutX="18.0" layoutY="175.0" prefHeight="318.0" prefWidth="374.0" />
      <Button fx:id="displayArtiste" layoutX="462.0" layoutY="232.0" mnemonicParsing="false" onAction="#displayArtiste" text="dispaly" textFill="#a45815">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Button fx:id="updateartiste" layoutX="459.0" layoutY="409.0" mnemonicParsing="false" onAction="#updateartiste" text="upadate" textFill="#a45815">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Button fx:id="deleteartiste" layoutX="464.0" layoutY="321.0" mnemonicParsing="false" onAction="#deleteartiste" text="delete" textFill="#a45815">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Pane prefHeight="130.0" prefWidth="600.0" style="-fx-background-color: #DAA06D;">
         <children>
            <ImageView fitHeight="196.0" fitWidth="242.0" layoutX="202.0" layoutY="-33.0" pickOnBounds="true" preserveRatio="true">
