<AnchorPane id="AnchorPane" prefHeight="495.0" prefWidth="644.0" stylesheets="@../CSS/yasser.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ModifierReservationController">
   <children>
      <Text layoutX="201.0" layoutY="271.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre de place">
         <font>
            <Font name="Bauhaus 93" size="15.0" />
         </font></Text>
      <TextField fx:id="nb_place" layoutX="325.0" layoutY="253.0" onAction="#nb_place" promptText="Saisie le nombre de place">
         <font>
            <Font name="Bauhaus 93" size="15.0" />
         </font></TextField>
      <Text layoutX="275.0" layoutY="324.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Prix">
         <font>
            <Font name="Bauhaus 93" size="15.0" />
         </font></Text>
      <TextField fx:id="price" layoutX="339.0" layoutY="306.0" onAction="#price" promptText="Saisie le prix">
         <font>
            <Font name="Bauhaus 93" size="15.0" />
         </font></TextField>
      <Button layoutX="358.0" layoutY="418.0" mnemonicParsing="false" onAction="#modifier" text="modifier" textFill="#b50505">
