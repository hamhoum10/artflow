<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MusqiueMM.FXMLMediaController">
   <center>
      <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="200.0" />
            <VBox alignment="BOTTOM_CENTER" minHeight="100.0" prefHeight="479.0" prefWidth="600.0">
               <children>
                  <Slider fx:id="progressBar">
                     <VBox.margin>
                        <Insets right="10.0" />
                     </VBox.margin>
                  </Slider>
                  <HBox prefHeight="121.0" prefWidth="600.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#chooseFileMethode" text="Choose File">
                           <HBox.margin>
                              <Insets left="7.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#play" prefHeight="26.0" prefWidth="58.0" text="Play">
                           <HBox.margin>
                              <Insets left="7.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#pause" prefHeight="26.0" prefWidth="54.0" text="Pause">
                           <HBox.margin>
                              <Insets left="7.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#stop" prefHeight="26.0" prefWidth="56.0" text="Stop">
                           <HBox.margin>
                              <Insets left="7.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#slowRate" prefHeight="26.0" prefWidth="46.0" text="&lt;&lt;&lt;">
                           <HBox.margin>
                              <Insets left="7.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#fastForward" prefHeight="26.0" prefWidth="48.0" text="&gt;&gt;&gt;">
                           <HBox.margin>
                              <Insets left="7.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#back10" prefHeight="26.0" prefWidth="80.0" text="-10S">
                           <HBox.margin>
                              <Insets left="7.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#skip10" prefHeight="26.0" prefWidth="56.0" text="+10S">
                           <HBox.margin>
                              <Insets left="7.0" right="5.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#exit" prefHeight="26.0" prefWidth="55.0" text="Exit" />
                       
                        <Slider fx:id="volumeSlider">
                           <HBox.margin>
                              <Insets top="7.0" />
                           </HBox.margin>
                        </Slider>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </center>
</BorderPane>
